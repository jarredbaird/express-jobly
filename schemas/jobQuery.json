{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.job.com/jobQuery.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "companyHandle": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "maxSalary": {
      "type": "string",
      "minimum": 0
    },
    "minSalary": {
      "type": "string",
      "minimum": 0
    },
    "minEquity": {
      "type": "string",
      "pattern": "0|(0?\\.[0-9]+)"
    },
    "maxEquity": {
      "type": "string",
      "pattern": "0|(0?\\.[0-9]+)"
    }
  },
  "additionalProperties": false,
  "required": []
}
